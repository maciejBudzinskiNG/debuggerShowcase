{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run and debug Android",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "skipFiles": [
        "node_modules/**",
        "internal/**",
        ".vscode/.react/debuggerWorker.js",
        "_stream_duplex.js",
        "_stream_passthrought.js",
        "_stream_readable.js",
        "_stream_transfrom.js",
        "_stream_writable.js",
        "async_hooks.js",
        "buffer.js",
        "child_process.js",
        "dgram.js",
        "events.js",
        "fs.js",
        "internal/**",
        "net.js",
        "path.js",
        "string_decoder.js",
        "timers.js",
        "url.js",
        "v8.js",
        "vm.js",
        "<eval>/**",
      ]
    },
    {
      "name": "Run and debug iOS Simulator",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "platform": "ios",
      "request": "launch",
      "skipFiles": [
        "node_modules/**",
        "internal/**",
        ".vscode/.react/debuggerWorker.js",
        "_stream_duplex.js",
        "_stream_passthrought.js",
        "_stream_readable.js",
        "_stream_transfrom.js",
        "_stream_writable.js",
        "async_hooks.js",
        "buffer.js",
        "child_process.js",
        "dgram.js",
        "events.js",
        "fs.js",
        "internal/**",
        "net.js",
        "path.js",
        "string_decoder.js",
        "timers.js",
        "url.js",
        "v8.js",
        "vm.js",
        "<eval>/**",
      ]
    },
    {
      "name": "Run and debug iOS Device",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "request": "launch",
      "platform": "ios",
      "target": "device=iPhone (Maciej) (13.3.1)",
      "skipFiles": [
        "node_modules/**",
        "internal/**",
        ".vscode/.react/debuggerWorker.js",
        "_stream_duplex.js",
        "_stream_passthrought.js",
        "_stream_readable.js",
        "_stream_transfrom.js",
        "_stream_writable.js",
        "async_hooks.js",
        "buffer.js",
        "child_process.js",
        "dgram.js",
        "events.js",
        "fs.js",
        "internal/**",
        "net.js",
        "path.js",
        "string_decoder.js",
        "timers.js",
        "url.js",
        "v8.js",
        "vm.js",
        "<eval>/**",
      ]
    },
    {
      "name": "Attach to the running packager",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "request": "attach",
      "skipFiles": [
        "node_modules/**",
        "internal/**",
        ".vscode/.react/debuggerWorker.js",
        "_stream_duplex.js",
        "_stream_passthrought.js",
        "_stream_readable.js",
        "_stream_transfrom.js",
        "_stream_writable.js",
        "async_hooks.js",
        "buffer.js",
        "child_process.js",
        "dgram.js",
        "events.js",
        "fs.js",
        "internal/**",
        "net.js",
        "path.js",
        "string_decoder.js",
        "timers.js",
        "url.js",
        "v8.js",
        "vm.js",
        "<eval>/**",
      ]
    }
  ]
}